{
	"name": "reactuiboilerplate",
	"version": "1.0.0",
	"description": "Microservices boilerplate template",
	"main": "index.js",
	"scripts": {
		"unit-test": "NODE_ENV=test ENV_SERVER=local node --max_old_space_size=4096 node_modules/nyc/bin/nyc.js node_modules/cucumber/bin/cucumber-js test/unit/features/ui/*.feature --format json:test/unit/reports/json/results.json ; npm run unit-test-report",
		"unit-test-report": "node test/unit/utils/json-to-html.js",
		"pact-publish": "ENV_PACT_BROKER_URL=http://10.10.10.10 node test/unit/utils/pact-publish.js",
		"pact-verify-provider": "ENV_PACT_BROKER_URL=http://10.10.10.10 ./node_modules/.bin/mocha --reporter mochawesome --reporter-options reportDir=test/cdc/reports,enableCode=false,reportFilename=index,reportPageTitle='CDC Pact Verify',inlineAssets=true --require ./test/unit/utils/helper.js --timeout 10000 test/cdc/provider.spec.js",
		"sonarqube-scanner": "node_modules/sonarqube-scanner/dist/bin/sonar-scanner -Dsonar.host.url=http://40.127.167.61:9000 -Dsonar.projectKey=react-ui-boilerplate -Dsonar.projectName=reactuiboilerplate -Dsonar.sources=src -Dsonar.javascript.lcov.reportPaths=test/unit/reports/coverage/lcov.info -Dsonar.exclusions=test/unit/reports/status/index.html",
		"uifunctional-test": "npm install -g protractor && webdriver-manager update && protractor test/functional/config.js ; node test/functional/json-to-html.js",
		"ui-perf-test-dev": "/usr/src/app/bin/sitespeed.js --config=test/performance/config.json test/performance/urls-dev.txt",
		"ui-perf-test-qa": "/usr/src/app/bin/sitespeed.js --config=test/performance/config.json test/performance/urls-qa.txt",
		"load-test": "npm install -g artillery && artillery run -k -o test/load/reports/report.json test/load/config.yaml",
		"load-test-report": "artillery report -o test/load/reports/index.html test/load/reports/report.json",
		"install-dependencies": "rm -rf node_modules && npm install --production",
		"pack-src": "mkdir -p scan && zip -r scan/reactuiboilerplate.zip src node_modules",
		"pack": "rm -rf node_modules && npm install --production && mkdir -p build && zip -r build/reactuiboilerplate.zip config dist node_modules package.json",
		"pack-vs": "npm install --production && mkdir -p build && zip -r build/reactuiboilerplate-vs.zip node_modules package.json test/virtual",
		"webpack": "webpack --config ./webpack.config.prod.js --progress",
		"start-vs": "npm install -g mountebank && mb --allowInjection --configfile ./test/virtual/Imposters.ejs",
		"install-pm2": "npm install pm2 -g",
		"build-dev": "npm install && node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.js --watch --progress -d",
		"build": "npm run webpack",
		"start-dev": "cross-env ENV_SERVER=local node dist/server-bundle.js",
		"start": "UV_THREADPOOL_SIZE=64 NODE_ENV=production node dist/server-bundle.js",
		"pm2-start": "pm2 start config/pm2.json --env production"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "6.24.1",
		"babel-core": "6.24.0",
		"babel-loader": "6.4.1",
		"babel-plugin-istanbul": "4.1.5",
		"babel-preset-es2015": "6.22.0",
		"babel-preset-es2015-loose": "8.0.0",
		"babel-preset-react": "6.23.0",
		"babel-preset-stage-0": "6.24.1",
		"chai": "3.5.0",
		"chai-as-promised": "6.0.0",
		"chai-http": "3.0.0",
		"clean-webpack-plugin": "0.1.16",
		"copy-webpack-plugin": "4.0.1",
		"cross-env": "5.0.0",
		"css-loader": "0.26.1",
		"cucumber": "4.2.1",
		"cucumber-html-reporter": "4.0.2",
		"enzyme": "2.8.2",
		"extract-text-webpack-plugin": "2.1.0",
		"jsdom": "9.9.0",
		"jsdom-global": "1.5.0",
		"jsonfile": "3.0.1",
		"mocha": "3.2.0",
		"mochawesome": "^2.0.5",
		"nock": "9.0.14",
		"nyc": "10.2.0",
		"optimize-css-assets-webpack-plugin": "3.0.0",
		"protractor-cucumber-framework": "5.0.0",
		"react-addons-test-utils": "15.5.1",
		"redux-mock-store": "1.2.3",
		"sinon": "2.3.6",
		"sonarqube-scanner": "2.0.2",
		"webpack": "2.2.1",
		"webpack-manifest-plugin": "1.1.0",
		"webpack-md5-hash": "0.0.5",
		"webpack-merge": "4.1.0",
