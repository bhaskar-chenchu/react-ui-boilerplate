		"response-time": "2.3.2",
		"rotating-file-stream": "1.2.2",
		"compression": "1.6.2",
		"morgan": "1.8.1",
		"body-parser": "1.17.1",
		"mobile-detect": "1.3.6",
		"html-to-json": "0.6.0",
		"lru-cache": "4.1.0",
		"axios-debug-log":"0.3.0",
		"winston-logrotate":"1.3.0",
		"replace-css-url": "1.0.4",
		"cache-manager":"2.4.0",
		"cache-manager-fs":"1.0.6",
		"node-schedule":"1.2.5",
		"fs-promise": "0.3.1",
		"swagger-jsdoc" : "1.9.7",
		"agentkeepalive" : "3.3.0",
		"heapdump":"0.3.9"
	}
}
