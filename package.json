		"cookie-parser": "1.4.3",
		"express": "4.15.2",
		"js-yaml": "3.11.0",
		"mongodb": "3.0.5",
		"react": "15.4.2",
		"react-dom": "15.4.2",
		"react-redux": "5.0.5",
		"redux": "3.7.1",
		"redux-thunk": "2.2.0",
		"serialize-javascript": "1.4.0",
		"underscore": "1.8.3",
		"axios": "0.15.3",
		"axios-cancel": "0.2.2",
